<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#7ecde5"/>
        </shape>
    </item>
	
    <item>
        <shape>
            <gradient
                android:startColor="#38c4f0"
                android:endColor="#7ecde5"
                android:angle="270"/>
        </shape>
    </item>
</selector>
